<!DOCTYPE html>
<html>
<head>
  <title>My URL Lists</title>
  <link rel="stylesheet" href="/css/site.css">
</head>
<body>
  <h2>My URL Lists</h2>
  <a href="/create">Create New List</a>
  <table border="1">
    <thead>
      <tr>
        <th>Title</th>
        <th>Custom URL</th>
        <th>Created</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% urlLists.forEach(function(list) { %>
        <tr>
          <td><%= list.title %></td>
          <td><a href="/view/<%= list.customUrl %>"><%= list.customUrl %></a></td>
          <td><%= list.createdAt.toLocaleDateString() %></td>
          <td><a href="/view/<%= list.customUrl %>">View</a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
