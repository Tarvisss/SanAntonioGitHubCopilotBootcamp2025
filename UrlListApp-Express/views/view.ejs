<!DOCTYPE html>
<html>
<head>
  <title><%= list.title %></title>
  <link rel="stylesheet" href="/css/site.css">
</head>
<body>
  <h2><%= list.title %></h2>
  <p><strong>Shareable URL:</strong> <code>/view/<%= list.customUrl %></code></p>
  <a href="/">Back to All Lists</a>
  <table border="1">
    <thead>
      <tr>
        <th>Title</th>
        <th>URL</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
      <% list.urls.forEach(function(item) { %>
        <tr>
          <td><%= item.title %></td>
          <td><a href="<%= item.url %>" target="_blank"><%= item.url %></a></td>
          <td><%= item.description %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
